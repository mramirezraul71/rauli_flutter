{
  "name": "RAULI Flutter Dev",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter",
        "GitHub.vscode-pull-request-github"
      ]
    }
  },
  "postCreateCommand": "bash .devcontainer/setup_flutter.sh"
}
